    String getFilePath(MultipartFile file) {
        Path FilePath = null;
        if (file.isEmpty()) {
            return "";
        } else {

            try {
                String date = simpleDateFormat.format(new Date());//get date 
                Timestamp timestamp = new Timestamp(System.currentTimeMillis());//get time
                String time = timestamp.getTime() + "";
                byte[] bytes = file.getBytes();
                FilePath = Paths.get(photoPath + date + " " + time + " " + file.getOriginalFilename());//crete file name
                Files.write(path, bytes);

            } catch (IOException e) {
                e.printStackTrace();
            }
        }
             return "";
    }
